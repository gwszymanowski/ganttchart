<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="ganttchart.controller.PrimaryController"
          >

    <top>
        <ganttchart.gui.elements.GanttMenu />
    </top>
    <center>
        <GridPane alignment="center" hgap="10" vgap="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <TableView fx:id="projectTable">
                <columns>
                    <TableColumn fx:id="nameColumn" text="Name" minWidth="250"/>
                    <TableColumn fx:id="optionsColumn" text="Options" minWidth="250" />
                </columns>
            </TableView>
        </GridPane>
    </center>

</BorderPane>